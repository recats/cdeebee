{
  "name": "@recats/cdeebee",
  "version": "1.3.1",
  "description": "React Redux data-logic library",
  "main": "lib/index.js",
  "repository": "git@github.com:recats/cdeebee.git",
  "author": "recats",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && yarn build-cjs && yarn build-umd && yarn build-esm",
    "build-cjs": "./node_modules/.bin/rollup src/index.js --config --file lib/index.js",
    "build-umd": "./node_modules/.bin/rollup src/index.js --config --format umd --file lib/index.umd.js",
    "build-esm": "./node_modules/.bin/rollup src/index.js --config --format esm --file lib/index.esm.js",
    "lint:js": "./node_modules/.bin/eslint ./src --ext=js,jsx",
    "test": "yarn lint:js"
  },
  "keywords": [
    "react",
    "redux",
    "axios",
    "data",
    "server"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-rewire-exports": "^1.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.0",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "nanoid": "^1.2.5",
    "ramda": "^0.25.0"
  }
}
