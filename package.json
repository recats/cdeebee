{
  "name": "@recats/cdeebee",
  "version": "3.0.0.beta-1",
  "description": "React Redux data-logic library",
  "repository": "git@github.com:recats/cdeebee.git",
  "author": "recats",
  "license": "MIT",
  "scripts": {
    "lint": "eslint lib/**/*.ts",
    "lint:ts": "tsc --noEmit --project tsconfig.lint.json",
    "lint:all": "pnpm lint && pnpm lint:ts",
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "react",
    "redux",
    "fetch",
    "data",
    "server"
  ],
  "type": "module",
  "main": "./lib/index.ts",
  "types": "./lib/index.ts",
  "exports": {
    ".": {
      "types": "./lib/index.ts",
      "import": "./lib/index.ts",
      "default": "./lib/index.ts"
    }
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@types/lodash": "^4.17.21",
    "@types/node": "^22.19.3",
    "@types/ramda": "^0.30.2",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.39.2",
    "jsdom": "^27.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": ">=2",
    "ramda": ">=0.27.1"
  },
  "packageManager": "pnpm@9.15.1"
}
